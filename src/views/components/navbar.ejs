<section class="container">
  <header class="tstbite-header bg-white">
    <nav class="navbar navbar-expand-lg has-header-inner px-0">
      <a class="navbar-brand" href="/">
        <img src="/images/brand.svg" style="max-width: 161px" alt="Tastebite" />
      </a>
      <div
        class="tstbite-header-links d-flex align-items-center ml-auto order-0 order-lg-2"
      >
        <div>
          <% if (session) { %>
          <div class="dropdown">
            <img
              src="https://cdn-icons-png.flaticon.com/512/149/149071.png"
              class="dropdown-toggle rounded-circle"
              width="50px"
              data-toggle="dropdown"
              aria-haspopup="true"
              aria-expanded="false"
              id="dropdownProfile"
              alt="Avatar"
            />
            <div
              class="dropdown-menu dropdown-menu-right p-1 pt-3"
              aria-labelledby="dropdownProfile"
              style="background-color: transparent; box-shadow: none"
            >
              <div
                style="
                  background-color: white;
                  border-radius: 10px;
                  box-shadow: 1px 1px 5px #ddd;
                  overflow: hidden;
                "
              >
                <a class="dropdown-item" href="#">My Profile</a>
                <a class="dropdown-item" href="#">Reset Password</a>
                <a class="dropdown-item" href="/logout">Logout</a>
              </div>
            </div>
          </div>
          <% } else { %>
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="btn btn-primary" href="/login">Login</a>
            </li>
          </ul>
          <% } %>
        </div>
      </div>
